/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=left);
    Or(a=a, b=b, out=right);
    And(a=left, b=right, out=out);
}
